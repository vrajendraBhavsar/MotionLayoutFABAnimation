{
  ConstraintSets: {
    start: {
      // on below line we are specifying width,height and margin
      // from start, top and end for img_hanuman_standing
      img_hanuman_standing: {
        width: 88,
        height: 88,
        end: ['parent', 'end', 16],
        bottom: ['parent', 'bottom', 16]
      },
      img_hanuman_pose: {
        width: 88,
        height: 88,
        end: ['parent', 'end', 16],
        bottom: ['parent', 'bottom', 16]
      }
    },
    end: {
      // on below line we are specifying width,height and margin
      // from start, top and end for img_hanuman_standing
      img_hanuman_standing: {
        width: 128,
        height: 128,
        start: ['parent', 'start'],
        end: ['parent', 'end'],
        top: ['parent', 'top'],
        bottom: ['parent', 'bottom']
      },
      img_hanuman_pose: {
        width: 128,
        height: 128,
        start: ['parent', 'start'],
        end: ['parent', 'end'],
        top: ['parent', 'top'],
        bottom: ['parent', 'bottom']
      }
    }
  },
  Transitions: {  //to set transition properties between Start and End point.
    default: {  //we'll have only one transition between Start and End. So we named it as Default. Your choice after all :D
      from: 'start',
      to: 'end',
      pathMotionArc: 'startHorizontal', // Compose view will move down with slight circular arc
      KeyFrames: {
        KeyAttributes: [ //We define different Attr and how we want this to Animate, during transition for specific composable
          {
            target: ['img_hanuman_standing'],
            frames: [0, 20, 100], //For frames we pass a List containing number between 0 - 100
            alpha: [1, 0.5, 0],
          },
          {
            target: ['img_hanuman_pose'],
            frames: [0, 50, 100], //For frames we pass a List containing number between 0 - 100
            alpha: [0, 0.5, 1]
          }
        ],
      }
    }
  }
}